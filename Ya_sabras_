int main()
{
 
int matrix[4] [4];
 
 for (int i=0; i<5; i++){// controlar filas
    for(int j=0; j<5; j++){
        matrix[i][j] = i*j;
    
    

     
  }
 
  
{
 
int matrix[5] [5];
 
 for (int i=0; i<5; i++){// controlar filas
    for(int j=0; j<5; j++){
        cout << matrix[i][j] << "|";
    }
    cout << endl;

     
  }
  
  return 0;

}








